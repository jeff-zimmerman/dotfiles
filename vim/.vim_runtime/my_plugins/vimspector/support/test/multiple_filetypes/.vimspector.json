{
  "configurations": {
    "Node": {
      "adapter": "js-debug",
      "filetypes": [ "javascript" ],
      "default": true,
      "configuration": {
        "request": "launch",
        "stopOnEntry": false,
        "console": "integratedTerminal",
        "program": "${workspaceRoot}/test.js",
        "cwd": "${workspaceRoot}"
      },
      "breakpoints": {
        "exception": {
          "all": "",
          "uncaught": ""
        }
      }
    },
    "Python": {
      "adapter": "debugpy",
      "filetypes": [ "python" ],
      "default": true,
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/test.py",
        "stopOnEntry": false,
        "cwd": "${workspaceRoot}"
      },
      "breakpoints": {
        "exception": {
          "raised": "N",
          "uncaught": "",
          "userUnhandled": ""
        }
      }
    }
  }
}
